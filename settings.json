  },
  "mdb.confirmRunAll": false,
  "sqltools.useNodeRuntime": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "newLine",
  "C_Cpp.formatting": "vcFormat",
  "debug.onTaskErrors": "debugAnyway",
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.intelliSenseUpdateDelay": 500,
  "C_Cpp.clang_format_sortIncludes": true,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "newLine",
  "python.analysis.typeCheckingMode": "basic",
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "window.confirmSaveUntitledWorkspace": false,
  "jest.runMode": "on-demand",
  "java.test.config": {},
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "security.promptForLocalFileProtocolHandling": false,
  "workbench.editorAssociations": {
    "{git,gitlens}:/**/*.{md,csv}": "default"
  },
  "workbench.editor.editorActionsLocation": "titleBar",
  "workbench.iconTheme": "vscode-icons",
  "material-icon-theme.activeIconPack": "none",
  "python.languageServer": "Pylance",
  "gitlens.views.branches.branches.layout": "list",
  "python.defaultInterpreterPath": "C:\\Users\\Raphael\\AppData\\Local\\Programs\\Python\\Python310\\python.exe",
  "gopls": {
    "ui.diagnostic.analyses": {},
    "formatting.gofumpt": true
  },
  "oneDarkPro.italic": false,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.leader": "<Space>",
  "vim.handleKeys": {
    "<C-h>": false,
    "<C-j>": false,
    "<C-k>": false,
    "<C-l>": false
  },
  "vim.easymotion": true,
  "vim.surround": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<S-k>"],
      "after": ["g", "h"]
    },
    {
      "before": ["<S-j>"],
      "after": ["nop"]
    },
    {
      "before": ["<C-c>"],
      "after": [":nohl"]
    },
    {
      "before": ["<A-k>"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["<A-j>"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["s"],
      "after": ["<leader>", "<leader>", "/"]
    },
    {
      "before": ["<leader>", "b", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "w", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "b", "o"],
      "commands": ["workbench.action.closeOtherEditors"]
    },
    {
      "before": ["<S-l>"],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": ["<S-h>"],
      "commands": ["workbench.action.previousEditorInGroup"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "s", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "c", "o"],
      "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["<leader>", "c", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "d", "b"],
      "commands": ["editor.debug.action.toggleBreakpoint"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<C-p>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<space>", "/"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["g", "I"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<C-space>"],
      "commands": ["editor.action.smartSelect.expand"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["<leader>", "w", "s"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["<leader>", "w", "v"],
      "commands": ["workbench.action.splitEditorRight"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-space>"],
      "commands": ["editor.action.smartSelect.expand"]
    },
    {
      "before": ["<BS>"],
      "commands": ["editor.action.smartSelect.shrink"]
    }
  ],
  "[templ]": {
    "editor.defaultFormatter": "a-h.templ"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "window.customTitleBarVisibility": "auto",
  "typescript.preferences.importModuleSpecifier": "relative",
  "keyboard.dispatch": "keyCode",
  "editor.tabSize": 2,
  "prettier.trailingComma": "all",
  "sql-formatter.uppercase": true,
  "editor.linkedEditing": true,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "markdown-preview-enhanced.liveUpdate": true,
  "git.openRepositoryInParentFolders": "always",
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "chat.editing.confirmEditRequestRemoval": false,
  "git.enableSmartCommit": true,
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "editor.tokenColorCustomizations": {
    "[One Dark Pro Darker]": {
      "textMateRules": [
        {
          "scope": ["variable", "variable.other.readwrite", "variable.other"],
          "settings": {
            "foreground": "#abb2bf"
          }
        },
        {
          "scope": [
            "variable.other.property",
            "support.type.property-name.json",
            "support.type.property-name.json punctuation"
          ],
          "settings": {
            "foreground": "#48b0bd"
          }
        },
        {
          "scope": ["variable.parameter"],
          "settings": {
            "foreground": "#E06C75"
          }
        }
      ]
    },
    "[One Dark Pro Night Flat]": {
      "textMateRules": [
        {
          "scope": ["variable", "variable.other.readwrite", "variable.other"],
          "settings": {
            "foreground": "#abb2bf"
          }
        },
        {
          "scope": [
            "variable.other.property",
            "support.type.property-name.json",
            "support.type.property-name.json punctuation"
          ],
          "settings": {
            "foreground": "#48b0bd"
          }
        },
        {
          "scope": ["variable.parameter"],
          "settings": {
            "foreground": "#E06C75"
          }
        }
      ]
    }
  },
  "java.inlayHints.parameterNames.enabled": "none",
  "workbench.colorTheme": "One Dark Pro Night Flat",
  "cursor.cpp.disabledLanguages": ["markdown", "scminput", "cpp"],
  "catppuccin.italicComments": false,
  "catppuccin.italicKeywords": false,
  "catppuccin.accentColor": "teal",
  "evenBetterToml.schema.associations": {}
}
