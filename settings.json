{
  "files.autoSave": "afterDelay",
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "open",
  "git.autofetch": true,
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "'jetbrainsmono nerd font', monospace",
  "editor.fontVariations": false,
  "editor.fontWeight": "700",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.maxTokenizationLineLength": 80000,
  "editor.hover.enabled": true,
  "zenMode.hideActivityBar": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "workbench.startupEditor": "none",
  "files.associations": {
    "*.tmpl": "html"
  },
  "emmet.includeLanguages": {
    "tmpl": "html",
    "html": "html",
    "javascript": "javascriptreact",
    "django-html": "html",
    "jinja-html": "html"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "git.confirmSync": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "javascript.format.semicolons": "insert",
  "codesnap.roundedCorners": false,
  "codesnap.showWindowControls": false,
  "codesnap.shutterAction": "copy",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.toolsManagement.autoUpdate": true,
  "python.analysis.autoImportCompletions": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "eslint.validate": ["javascript"],
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "css.validate": false,
  "editor.inlineSuggest.enabled": true,
  "python.autoComplete.extraPaths": [],
  "python.analysis.extraPaths": [],
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "yaml": true,
    "cpp": false
  },
  "git.ignoreMissingGitWarning": true,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "terminal.integrated.fontWeightBold": "bold",
  "remote.SSH.remotePlatform": {
    "sandbox.69mlh5.csb": "linux"
  },
  "redhat.telemetry.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.cursorSurroundingLines": 10,
  "github.copilot.advanced": {},
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "terminal.integrated.fontFamily": "Jetbrainsmono nerd font",
  "svelte.enable-ts-plugin": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vscode-office.openOutline": false,
  "gitlens.graph.minimap.enabled": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "mdb.confirmRunAll": false,
  "sqltools.useNodeRuntime": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "newLine",
  "C_Cpp.formatting": "vcFormat",
  "debug.onTaskErrors": "debugAnyway",
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.intelliSenseUpdateDelay": 500,
  "C_Cpp.clang_format_sortIncludes": true,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "newLine",
  "python.analysis.typeCheckingMode": "standard",
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "window.confirmSaveUntitledWorkspace": false,
  "jest.runMode": "on-demand",
  "java.test.config": {},
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "security.promptForLocalFileProtocolHandling": false,
  "workbench.editorAssociations": {
    "{git,gitlens}:/**/*.{md,csv}": "default"
  },
  "workbench.editor.editorActionsLocation": "titleBar",
  "workbench.iconTheme": "vscode-icons",
  "material-icon-theme.activeIconPack": "none",
  "editor.lineNumbers": "relative",
  "vscode-office.editorTheme": "Auto",
  "python.languageServer": "Pylance",
  "gitlens.views.branches.branches.layout": "list",
  "python.defaultInterpreterPath": "C:\\Users\\Raphael\\AppData\\Local\\Programs\\Python\\Python310\\python.exe",
  "gopls": {
    "ui.diagnostic.analyses": {},
    "formatting.gofumpt": true
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vscode-neovim.completionDelay": 500,
  "catppuccin.italicKeywords": false,
  "oneDarkPro.italic": false,
  "prisma.showPrismaDataPlatformNotification": false,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.leader": "<Space>",
  "vim.handleKeys": {},
  "vim.easymotion": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<S-k>"],
      "after": ["g", "h"]
    },
    {
      "before": ["<S-j>"],
      "after": ["nop"]
    },
    {
      "before": ["<C-c>"],
      "after": [":nohl"]
    },
    {
      "before": ["<A-k>"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["<A-j>"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["s"],
      "after": ["<leader>", "<leader>", "/"]
    },
    {
      "before": ["<leader>", "b", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "b", "o"],
      "commands": ["workbench.action.closeOtherEditors"]
    },
    {
      "before": ["<S-l>"],
      "commands": ["workbench.action.nextEditorInGroup"]
    },
    {
      "before": ["<S-h>"],
      "commands": ["workbench.action.previousEditorInGroup"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "s", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "c", "o"],
      "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["<leader>", "c", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "d", "b"],
      "commands": ["editor.debug.action.toggleBreakpoint"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<C-p>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<space>", "/"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["g", "I"],
      "commands": ["editor.action.peekImplementation"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<C-space>"],
      "commands": ["editor.action.smartSelect.expand"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-space>"],
      "commands": ["editor.action.smartSelect.expand"]
    },
    {
      "before": ["<BS>"],
      "commands": ["editor.action.smartSelect.shrink"]
    }
  ],
  "[templ]": {
    "editor.defaultFormatter": "a-h.templ"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "window.customTitleBarVisibility": "auto",
  "typescript.preferences.importModuleSpecifier": "relative"
}
