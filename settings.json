{
  "files.autoSave": "afterDelay",
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "open",
  "git.autofetch": true,
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "'jetbrainsmono nerd font', monospace",
  "editor.fontVariations": false,
  "editor.fontWeight": "700",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.maxTokenizationLineLength": 80000,
  "editor.hover.enabled": true,
  "zenMode.hideActivityBar": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "workbench.startupEditor": "none",
  "files.associations": {
    "*.tmpl": "html"
  },
  "emmet.includeLanguages": {
    "tmpl": "html",
    "html": "html",
    "javascript": "javascriptreact",
    "django-html": "html",
    "jinja-html": "html"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "git.confirmSync": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "javascript.format.semicolons": "insert",
  "codesnap.roundedCorners": false,
  "codesnap.showWindowControls": false,
  "codesnap.shutterAction": "copy",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.toolsManagement.autoUpdate": true,
  "python.analysis.autoImportCompletions": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "eslint.validate": ["javascript"],
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "css.validate": false,
  "editor.inlineSuggest.enabled": true,
  "python.autoComplete.extraPaths": [],
  "python.analysis.extraPaths": [],
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "yaml": true,
    "cpp": false
  },
  "git.ignoreMissingGitWarning": true,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "terminal.integrated.fontWeightBold": "bold",
  "remote.SSH.remotePlatform": {
    "sandbox.69mlh5.csb": "linux"
  },
  "redhat.telemetry.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.cursorSurroundingLines": 10,
  "github.copilot.advanced": {},
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "terminal.integrated.fontFamily": "Jetbrainsmono nerd font",
  "svelte.enable-ts-plugin": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vscode-office.openOutline": false,
  "gitlens.graph.minimap.enabled": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "mdb.confirmRunAll": false,
  "sqltools.useNodeRuntime": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "newLine",
  "C_Cpp.formatting": "vcFormat",
  "debug.onTaskErrors": "debugAnyway",
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.intelliSenseUpdateDelay": 500,
  "C_Cpp.clang_format_sortIncludes": true,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "newLine",
  "python.analysis.typeCheckingMode": "basic",
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "window.confirmSaveUntitledWorkspace": false,
  "jest.runMode": "on-demand",
  "java.test.config": {},
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "security.promptForLocalFileProtocolHandling": false,
  "workbench.editorAssociations": {
    "{git,gitlens}:/**/*.{md,csv}": "default"
  },
  "workbench.editor.editorActionsLocation": "titleBar",
  "workbench.iconTheme": "material-icon-theme",
  "window.customTitleBarVisibility": "auto",
  "material-icon-theme.activeIconPack": "none",
  "editor.lineNumbers": "relative",
  "vscode-office.editorTheme": "Auto",
  "python.languageServer": "Pylance",
  "gitlens.views.branches.branches.layout": "list",
  "gopls": {
    "ui.diagnostic.analyses": {
      "structtag": true
    }
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.colorTheme": "One Dark Pro Darker"
}
